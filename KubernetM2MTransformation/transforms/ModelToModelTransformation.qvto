modeltype UML uses 'http://www.eclipse.org/uml2/5.0.0/UML';
modeltype Kubernetes uses 'kube_metamodel1';


transformation ModelToModelTransformation(in dd : UML, out traces : Kubernetes);

main() {
(dd.rootObjects())[UML::Model]->asOrderedSet()->first().map deployment2infra();



}


mapping UML::NamedElement::deployment2infra() : Kubernetes::Infrastructure {
	name := self.name;
}

