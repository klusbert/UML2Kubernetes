    <eStructuralFeatures xsi:type="ecore:EAttribute" name="identifier" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataPersistent" eType="#//PersistentData"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="namespace" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ingress" eType="#//Ingress"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="securityContext" eType="#//SecurityContext"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="resourceAllocation" eType="#//ResourceAllocation"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EnviromentVariables">
